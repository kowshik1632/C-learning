#include<stdio.h>
main()
{
	int marks ;
	printf("Enter the marks: ");
	scanf("%d",&marks);
	if(marks<0 || marks>100)
	{
		printf("Wrong entry");
	}
	else if(marks>=85) 
	{
		printf("Grade A");
	}
	else if(marks>=70)
	{
		printf("Grade B");
	}
	else if(marks>=55)
	{
		printf("Grade C");
	}
	else if(marks>=40)
	{
		printf("Grade D");
	}
	else
	{
		printf("Grade F");
	}
}
